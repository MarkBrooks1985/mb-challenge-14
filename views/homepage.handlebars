<section id="blog">
    {{#each blog as |blog|}}
    <div>
        <a href="'blog/{{blog.id}}'">
        <div>
            <div>
                <h1>{{blog.title}}</h1>
                <h2>Author: {{blog.user}}</h2>
                <p>{{blog.content}}</p>
            </div>
        </div></a>
    </div>
    {{/each}}
</section>